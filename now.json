{
    "name": "subc",
    "version": 2, 
    "builds": [
        {
            "src": "subconverter/subconverter.exe",
            "use": "vercel-static-bin",
            "config": {
                "port": 25500,
                "timeout": 1500,
                "includeFiles": [
                    "subconverter/**"
                ]
            }
        }
    ],
    "routes": [
        {"src": "/(.*)", "dest": "subconverter/subconverter.exe"}
    ]
}
